<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>ShowServlet</servlet-name>
    <servlet-class>com.item.servlet.ShowServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowServlet</servlet-name>
    <url-pattern>/ShowServlet.do</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>InsertOrderServlet</servlet-name>
    <servlet-class>com.item.servlet.InsertOrderServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ShowOrderServlet</servlet-name>
    <servlet-class>com.item.servlet.ShowOrderServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>InsertOrderServlet</servlet-name>
    <url-pattern>/InsertOrderServlet.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowOrderServlet</servlet-name>
    <url-pattern>/ShowOrderServlet.do</url-pattern>
  </servlet-mapping>
  
  
  
  	
  <welcome-file-list>
    <welcome-file>ShowServlet.do</welcome-file>
  </welcome-file-list>
</web-app>
